<UserControl x:Class="TCMS.GUI.Views.IncidentLog"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TCMS.GUI.Views"
      xmlns:viewModels="clr-namespace:TCMS.GUI.ViewModels"
      mc:Ignorable="d" 
      d:DesignHeight="600"
      d:DesignWidth="622"
      Style="{StaticResource Page_Style}">


    <UserControl.DataContext>
        <viewModels:IncidentLogViewModel />
    </UserControl.DataContext>


    <Grid>

        <Border CornerRadius="0,20,20,0" Margin="0,5,5,0">
        <Border.Background>
            <ImageBrush ImageSource="/Assets/Images/shipment_page.png"
                            Stretch="None" />
        </Border.Background>
    </Border>

    </Grid>
</UserControl>
